<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">
<suite name="Suite" parallel="false">

	<listeners>

		<listener
			class-name="com.demo.testreport.ExtentReportITestListener" />
	</listeners>


	<test name="Api">
		<groups>
			<run>
				<include name="Country"></include>
			</run>
		</groups>
		<classes>
			<class name="com.demo.test.api.ApiGetTest">
				<methods>
					<include name="getAllCountriesTest"></include>
					<include name="getCountryByIdTest"></include>
					<exclude name="createNewCountryTest"></exclude>
				</methods>
			</class>
		</classes>
	</test>

</suite> 
